<% include ./partials/header.ejs%>
 <div class="w-full h-full bg-zinc-800 px-10 py-8 text-white" >
    <div class="card flex flex-wrap gap-10">
        <% posts.forEach(function(elem){%>


     
        <div class="card  w-32">
        <div class=" w-full h-40 bg-zinc rounded-lg overflow-hidden">
            <img class="w-full h-full object-cover ml-[4vh]" src="/images/uploads/<%=elem.image%>" class="w-full h-full object-cover" alt="">
        </div>
        <h5  class="inline-block text-md font-semibold mt-2">@<%=elem.user.username %></h5>
        <h5 class="mt-1 text-sm"><%=elem.title%></h5>
    </div>
<%    }) %>
    </div>
 </div>
